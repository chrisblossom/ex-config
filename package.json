{
  "name": "ex-config",
  "version": "1.0.11",
  "description": "Extendable configuration processor.",
  "keywords": [
    "config",
    "configuration",
    "extend",
    "extendable",
    "cosmiconfig",
    "rc",
    "resolve",
    "prefix",
    "require",
    "resolve string",
    "require string",
    "string config",
    "string configuration"
  ],
  "license": "MIT",
  "repository": "chrisblossom/ex-config",
  "homepage": "https://github.com/chrisblossom/ex-config",
  "author": "Chris Blossom",
  "engines": {
    "node": ">=6.9.0",
    "npm": ">=3.10.10"
  },
  "files": [
    "dist/",
    "flow-typed/"
  ],
  "main": "dist/ex-config.js",
  "scripts": {
    "build": "backtrack build --production",
    "check-all": "backtrack check-all",
    "clean": "backtrack clean",
    "dev": "backtrack dev --development",
    "flow": "backtrack flow",
    "format": "backtrack format",
    "lint": "backtrack lint",
    "lint.fix": "backtrack lint.fix",
    "precommit": "backtrack precommit",
    "prepublishOnly": "backtrack prepublishOnly",
    "prepush": "backtrack prepush",
    "test": "backtrack test",
    "test.ci": "backtrack test.ci",
    "test.ci-pretest": "backtrack test.ci-pretest",
    "test.update": "jest --updateSnapshot",
    "test.watch": "jest --watch"
  },
  "lint-staged": {
    "*.{js,md,json}": [
      "prettier --write",
      "git add"
    ]
  },
  "babel": {
    "presets": [
      "./.babelrc.js"
    ]
  },
  "devDependencies": {
    "@backtrack/core": "^0.1.20",
    "@backtrack/preset-node-module": "^0.2.1"
  },
  "dependencies": {
    "joi": "^12.0.0",
    "lodash.clonedeep": "^4.5.0",
    "lodash.isplainobject": "^4.0.6",
    "lodash.mergewith": "^4.6.1",
    "resolve-with-prefix": "^1.0.7"
  }
}
